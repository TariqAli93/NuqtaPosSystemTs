{
  "name": "@nuqtaplus/electron",
  "version": "1.0.0",
  "main": "./out/main/index.js",
  "types": "./out/main/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./out/main/index.d.ts",
      "default": "./out/main/index.js"
    }
  },
  "scripts": {
    "rebuild:sqlite": "electron-rebuild -f -w better-sqlite3 --module-dir ../../packages/data",
    "dev": "pnpm run rebuild:sqlite && electron-vite dev --watch",
    "build": "pnpm run rebuild:sqlite && electron-vite build",
    "dist": "pnpm run rebuild:sqlite && electron-builder --config electron-builder.yml",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test:unit": "vitest run src/__tests__"
  },
  "dependencies": {
    "@nuqtaplus/core": "workspace:*",
    "@nuqtaplus/data": "workspace:*",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.4",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "vite-plugin-vuetify": "^2.1.3",
    "better-sqlite3": "^11.10.0",
    "drizzle-orm": "^0.45.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.3",
    "@types/node": "^20.11.19",
    "autoprefixer": "^10.4.24",
    "electron": "^31.7.7",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^5.0.0",
    "postcss": "^8.5.6",
    "sass-embedded": "^1.97.3",
    "vite-plugin-vue-devtools": "^8.0.6",
    "electron-builder": "^25.1.8",
    "vitest": "^1.2.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.0"
}
